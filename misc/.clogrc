# Standard syslog entries.
default rule /[Ww]arn|[Dd]ebug/   --> yellow line
default rule /[Ee]rror|[Ss]evere/ --> red line
default rule /[Cc]ritical/     --> bold red line
default rule /[Cc]ritical/     --> blank
default rule /[Ii]gnore/       --> suppress

# Apache access.log status codes
apache rule / 2[0-9][0-9] / --> green match
apache rule / 3[0-9][0-9] / --> yellow match
apache rule / 4[0-9][0-9] / --> red match
apache rule / 5[0-9][0-9] / --> bold red match

#
default rule /rtkit-daemon/    --> bold red line
default rule /bluetoothd/    --> bold blue line
default rule /blueman/    --> bold blue line
default rule /laptop[-_]+mode/    --> bold blue line
default rule /kernel:/      --> yellow line
default rule /root:/      --> bold yellow line
default rule /rsyslogd:/      --> bold black line
default rule /ntpdate/      --> bold green line
default rule /systemd-timesyncd\[[0-9]+\]:/      --> bold green line
default rule /systemd\[[0-9]+\]:/      --> bold red match
default rule /dbus-daemon/  --> red line
default rule /PackageKit:/      --> yellow line
default rule /CRON/      --> yellow line
default rule /dhclient\[[0-9]+\]:/      --> bold red match
default rule /NetworkManager\[[0-9]+\]:/      --> bold yellow match
default rule /nm-dispatcher:/      --> bold yellow match
default rule /whoopsie\[[0-9]+\]:/      --> bold magenta match

default rule /[Ee]rror/     --> bold red line
default rule /[Ff]ailed/    --> bold red line
default rule /BOOT_IMAGE/   --> bold green line
default rule /[Ss]ucceeded/ --> bold green match
default rule /[Ss]uccess/ --> bold green match
default rule /[Ss]uccessfully/ --> bold green match
default rule /[Aa]ctivated/ --> bold green match
default rule /[Aa]ctivating service name/ --> bold green match
default rule /'.*'/ --> bold cyan match
default rule /is enabled/ --> bold green match
default rule /[0-9.]+Gbps/ --> bold green match
default rule /[0-9.]+ Gbps/ --> bold green match
default rule /[Ll]ink is [Uu]p/ --> bold green match
default rule /[Ll]ink [Uu]p/ --> bold green match

